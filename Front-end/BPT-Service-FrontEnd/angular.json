{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "BPT-Service-FrontEnd": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/BPT-Service-FrontEnd",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "../BPT-Service-FrontEnd/node_modules/bootstrap/dist/css/bootstrap.min.css",
              "../BPT-Service-FrontEnd/node_modules/font-awesome/css/font-awesome.css",
              "../BPT-Service-FrontEnd/node_modules/animate.css/animate.css",
              "../BPT-Service-FrontEnd/node_modules/alertifyjs/build/css/alertify.css",
              "../BPT-Service-FrontEnd/node_modules/alertifyjs/build/css/themes/default.css",
              "../BPT-Service-FrontEnd/src/assets/css/custom.css",
              "../BPT-Service-FrontEnd/node_modules/angular-tree-component/dist/angular-tree-component.css",
              "src/styles.css"
            ],
            "scripts": [
              "../BPT-Service-FrontEnd/node_modules/jquery/dist/jquery.min.js",
              "../BPT-Service-FrontEnd/node_modules/popper.js/dist/umd/popper.min.js",
              "../BPT-Service-FrontEnd/node_modules/bootstrap/dist/js/bootstrap.min.js",
              "../BPT-Service-FrontEnd/node_modules/fastclick/lib/fastclick.js",
              "../BPT-Service-FrontEnd/node_modules/alertifyjs/build/alertify.js",
              "../BPT-Service-FrontEnd/node_modules/nprogress/nprogress.js",
              "../BPT-Service-FrontEnd/node_modules/moment/moment.js",
              "../BPT-Service-FrontEnd/node_modules/chart.js/dist/Chart.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "BPT-Service-FrontEnd:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "BPT-Service-FrontEnd:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "BPT-Service-FrontEnd:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "BPT-Service-FrontEnd:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "BPT-Service-FrontEnd:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "BPT-Service-FrontEnd"
}